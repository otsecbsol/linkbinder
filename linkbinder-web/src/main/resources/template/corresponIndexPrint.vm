#end
#if($util.isView("to"))
        <th class="header" style="text-align:center;white-space:nowrap;">宛先</th>
#end
#if($util.isView("type"))
        <th class="header" style="text-align:center;white-space:nowrap;">種類</th>
#end
#if($util.isView("subject"))
        <th class="header" style="text-align:center;white-space:nowrap;">件名</th>
#end
#if($util.isView("workflow"))
