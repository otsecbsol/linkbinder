#end
#if($util.isView("status"))
        <th class="header" style="text-align:center;white-space:nowrap;">状態</th>
#end
      </tr>
#set($row = true)
#foreach($correspon in $data)
#if($util.isCanceled($correspon.corresponStatus))
      <tr class="canceled">
#elseif($util.isDeadline($correspon.deadlineForReply))
      <tr class="deadline">
#elseif($row)
      <tr class="odd">
#else
      <tr class="even">
#end
#if($correspon.corresponReadStatus.returnedUnread)
        <td class="unread">*</td>
#else
        <td></td>
#end
#if($util.isView("no"))
        <td class="number">$util.getUnreadStyle($util.toString($correspon.id),
                               $correspon.corresponReadStatus.readStatus)</td>
#end
#if($util.isView("corresponNo"))
